{
  "name": "Amazon Seller Reverse Search",
  "manifest_version": 2,
  "version": "0.2",
  "icons": {
    "256": "icons/icon.png"
  },
  "description": "Send current seller to google sheets",
  "author": "Amaf Jarkasi",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Save seller",
    "default_icon": "icons/icon.png"
  },
  "permissions": [
    "*://*/*",
    "notifications",
    "tabs"
  ]
}